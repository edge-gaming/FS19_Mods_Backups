        <specs>
            <combination>AGCO 650</combination>
			<neededPower>50</neededPower>
        </specs>
        <functions>
            <function>$l10n_typeDesc_EDGEroller</function>
        </functions>
        <image>store_EDGE_Roller_Pro.dds</image>
        <price>4900</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>EDGE</brand>
        <category>misc</category>
        <shopTranslationOffset>-0.5 0 -1.5</shopTranslationOffset>
