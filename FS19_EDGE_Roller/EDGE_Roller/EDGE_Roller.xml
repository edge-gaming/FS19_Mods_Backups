    </storeData>
    <base>
        <typeDesc>$l10n_typeDesc_roller</typeDesc>
        <filename>EDGE_Roller/EDGE_Roller.i3d</filename>
        <size width="6" length="2"/>
        <speedLimit value="17"/>
        <components>
            <component centerOfMass="0 0.1 0" solverIterationCount="10" mass="1980"/>
        </components>
        <schemaOverlay attacherJointPosition="0 0">
            <default name="DEFAULT_IMPLEMENT"/>
            <selected name="DEFAULT_IMPLEMENT_SELECTED"/>
        </schemaOverlay>
    </base>
    <baseMaterial>
        <material name="EDGE_Roller_Pro_mat" baseNode="Attacher">
            <shaderParameter name="colorMat0" value="STOLL_GREY1"/>
        </material>
        <material name="RollerLite_mat" baseNode="LiteDrum">
            <shaderParameter name="colorMat2" value="DUEVELSDORF_RED1"/>
        </material>
        <material name="RollerPro_mat" baseNode="ProDrum">
            <shaderParameter name="colorMat2" value="DUEVELSDORF_RED1"/>
        </material>
    </baseMaterial>
    <baseMaterialConfigurations price="100" useDefaultColors="true">
        <baseMaterialConfiguration color="STOLL_GREY1"/>
        <baseMaterialConfiguration name="SILVER" color="0.6456 0.6456 0.6456 1" material="8" price="100"/>
        <baseMaterialConfiguration name="DARK BLUE CHROME" color="0.007 0.029 0.187 1" material="2" price="100"/>
        <baseMaterialConfiguration name="CHROME" color="0.9800 0.9800 0.9800 1" material="2" price="100"/>
        <baseMaterialConfiguration name="DARK GREY" color="0.037 0.037 0.037 1" material="0" price="100"/>
        <baseMaterialConfiguration name="ORANGE CHROME" color="0.932 0.119 0.000 1" material="2" price="100"/>
        <baseMaterialConfiguration name="GREEN CHROME" color="0.000 0.170 0.001 1" material="2" price="100"/>
        <baseMaterialConfiguration name="BLUE CHROME" color="0.062 0.251 0.612 1" material="2" price="100"/>
        <baseMaterialConfiguration name="RED CHROME" color="0.673 0.002 0.000 1" material="2" price="100"/>
        <material name="EDGE_Roller_Pro_mat" shaderParameter="colorMat0"/>
    </baseMaterialConfigurations>
    <designMaterialConfigurations price="100" useDefaultColors="true">
        <designMaterialConfiguration color="DUEVELSDORF_RED1"/>
        <designMaterialConfiguration name="SILVER" color="0.6456 0.6456 0.6456 1" material="8" price="100"/>
        <designMaterialConfiguration name="DARK BLUE CHROME" color="0.007 0.029 0.187 1" material="2" price="100"/>
        <designMaterialConfiguration name="CHROME" color="0.9800 0.9800 0.9800 1" material="2" price="100"/>
