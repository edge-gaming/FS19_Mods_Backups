        <designMaterialConfiguration name="BLUE CHROME" color="0.062 0.251 0.612 1" material="2" price="100"/>
        <designMaterialConfiguration name="RED CHROME" color="0.673 0.002 0.000 1" material="2" price="100"/>
        <material name="RollerPro_mat" shaderParameter="colorMat2"/>
        <material name="RollerLite_mat" shaderParameter="colorMat2"/>
    </designMaterialConfigurations>
    <designConfigurations>
        <designConfiguration name="Lite" >
            <objectChange node="ProDrum" visibilityActive="false"/>
            <objectChange node="LiteDrum" visibilityActive="true"/>
        </designConfiguration>
		<designConfiguration name="Pro" price="100">
            <objectChange node="LiteDrum" visibilityActive="false"/>
            <objectChange node="ProDrum" visibilityActive="true"/>
        </designConfiguration>
    </designConfigurations>
    <wheels>
        <wheelConfigurations>
            <wheelConfiguration name="$l10n_configuration_valueDefault" price="0">
                <wheels>
                    <wheel isLeft="false" hasParticles="true">
                        <physics restLoad="1.0" repr="wheelDrum" radius="0.468" mass="0.2" width="3" forcePointRatio="0.5" initialCompression="30" suspTravel="0.05" spring="60" damper="20" rotationDamping="0.002"/>
                    </wheel>
                </wheels>
            </wheelConfiguration>
        </wheelConfigurations>
    </wheels>
    <speedRotatingParts>
        <speedRotatingPart node="LiteDrum" radius="0.468" wheelIndex="1"/>
        <speedRotatingPart node="ProDrum" radius="0.468" wheelIndex="1"/>
    </speedRotatingParts>
    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="implement" topReferenceNode="topReferenceNode">
                <distanceToGround lower="0.63" upper="1.2"/>
            </inputAttacherJoint>
        </inputAttacherJoints>
        <support animationName="moveSupport"/>
        <brakeForce>0.5</brakeForce>
    </attachable>
    <groundReferenceNodes>
        <groundReferenceNode node="groundReferenceNode" threshold="0.3" chargeValue="1"/>
    </groundReferenceNodes>
    <workAreas>
        <workArea type="roller" functionName="processRollerArea" disableBackwards="false">
            <area startNode="workAreaStart" widthNode="workAreaWidth" heightNode="workAreaHeight"/>
            <groundReferenceNode index="1"/>
        </workArea>
    </workAreas>
	<foldable>
        <foldingConfigurations title="$l10n_Config_Size">
            <foldingConfiguration name="$l10n_Config_3M" price="0" workingWidth="3" >
				<objectChange node="EDGERollerSvis" visibilityInactive="false" visibilityActive="true" />
				<objectChange node="supportCol" compoundChildInactive="false" compoundChildActive="true" />
				<objectChange node="EDGERollerM" compoundChildInactive="true" compoundChildActive="false" />
				<objectChange node="supportColM" compoundChildInactive="true" compoundChildActive="false" />
				<objectChange node="EDGERollerL" compoundChildInactive="true" compoundChildActive="false" />
				<objectChange node="supportColL" compoundChildInactive="true" compoundChildActive="false" />
			</foldingConfiguration>
            <foldingConfiguration name="$l10n_Config_4M" price="2000" workingWidth="4" >
                <objectChange node="EDGERollerMvis" visibilityInactive="false" visibilityActive="true" />
				<objectChange node="Support" translationActive="0.372 0 0" translationInactive="0 0 0" />
				<objectChange node="supportColM" translationActive="-0.372 0.241 0" translationInactive="0 0.241 0" />
				<objectChange node="wheels" scaleActive="1.34 1 1" scaleInactive="1 1 1" />
				<objectChange node="workArea" scaleActive="1.34 1 1" scaleInactive="1 1 1" />
				<objectChange node="EDGERollerM" compoundChildInactive="false" compoundChildActive="true" />
				<objectChange node="supportColM" compoundChildInactive="false" compoundChildActive="true" />
            </foldingConfiguration>
			<foldingConfiguration name="$l10n_Config_6M" price="4000" workingWidth="6" >
                <objectChange node="EDGERollerLvis" visibilityInactive="false" visibilityActive="true" />
                <objectChange node="SupportL" visibilityInactive="false" visibilityActive="true" />
				<objectChange node="Support" translationActive="1.45 0 0" translationInactive="0 0 0" />
				<objectChange node="supportColL" translationActive="-1.45 0.241 0" translationInactive="0 0.241 0" />
				<objectChange node="wheels" scaleActive="2 1 1" scaleInactive="1 1 1" />
				<objectChange node="workArea" scaleActive="2 1 1" scaleInactive="1 1 1" />
				<objectChange node="EDGERollerL" compoundChildInactive="false" compoundChildActive="true" />
				<objectChange node="supportColL" compoundChildInactive="false" compoundChildActive="true" />
            </foldingConfiguration>
        </foldingConfigurations>
    </foldable>
    <animations>
        <animation name="moveSupport">
            <part node="supportFoot" startTime="0" endTime="0.2" startTrans="0 0.28 0" endTrans="0 0 0"/>
			<part node="supportFootL" startTime="0" endTime="0.2" startTrans="0 0.28 0" endTrans="0 0 0"/>
        </animation>
    </animations>
    <roller onlyActiveWhenLowered="true">
        <sounds>
            <work template="MEADOW_ROLLER_WORK"/>
        </sounds>
    </roller>
    <foliageBending>
        <bendingNode minX="-2.0" maxX="2.0" minZ="-0.5" maxZ="0.5" yOffset="0.46"/>
    </foliageBending>
	<wearable wearDuration="1000" workMultiplier="5" fieldMultiplier="2"/>
    <washable dirtDuration="30" washDuration="1" workMultiplier="10" fieldMultiplier="20"/>
